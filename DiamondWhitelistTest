local Players = game:GetService("Players")

_G.Noclip = false
spawn(function()
    game:GetService("RunService").Heartbeat:Connect(function()
        pcall(function ()
            if _G.Noclip then
                game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled(11,false)
            end

            if _G.Noclip then
                game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled(11,true)
            end
        end)
    end)
end)

function TP(P1)
	Distance = (P1.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
	if Distance < 1000 then
		Speed = 400
	elseif Distance >= 1000 then
		Speed = 250
	end
    local twer = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart,TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear),
        {CFrame = P1}):Play()
    _G.Noclip = true
    wait(Distance/Speed)
end

function BypassTP(C1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = C1
end

getgenv().autobypass = nil
_G.Bypassed = nil
function iv()
	pcall(function ()
		Enemies = workspace.Enemies
		Rep = game.ReplicatedStorage

		while task.wait() do
			if getgenv().autobypass then
				if _G.Bypassed == nil then
					game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled(15, false)
					if world1 then
						EnMob = Enemies:FindFirstChild("Military Spy [Lv. 330]")
						RepMob = Rep:FindFirstChild("Military Spy [Lv. 330]")
					elseif world2 then
						EnMob = Enemies:FindFirstChild("Marine Captain [Lv. 900]")
						RepMob = Rep:FindFirstChild("Marine Captain [Lv. 900]")
					elseif world3 then
						EnMob = Enemies:FindFirstChild("Marine Commodore [Lv. 1700]")
						RepMob = Rep:FindFirstChild("Marine Commodore [Lv. 1700]")
					end
	
					Mob = EnMob or RepMob
	
					if game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
						repeat task.wait()
							BypassTP(Mob.HumanoidRootPart.CFrame * CFrame.new(0, 0, -3))
						until
							not getgenv().autobypass or not _G.Bypassed == false or game.Players.LocalPlayer.Character.Humanoid.Health <= 0
					else
						game.Players.LocalPlayer.Character.Humanoid.Health = game.Players.LocalPlayer.Character.Humanoid.MaxHealth
						_G.Bypassed = true
					end
				end
			end
		end
	end)	
end

function BypassEnabled()
        game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled(15,false);
        game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled(16,false);
        game.Players.LocalPlayer.Character.Humanoid.Health = math.huge
		getgenv().autobypass = true
	if world2 then
    iv()
	elseif world3 then
    iv()
	elseif world1 then
    iv()
	end
end

function BypassDisibled()
        game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled(15,true);
        game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled(16,true);
        game.Players.LocalPlayer.Character.Humanoid.Health = 0
		getgenv().autobypass = nil
end

