-------------------------------------------------Misc-------------------------------------------------
_G.FastAttack = false
local Page5 = Tab3:addpage()

spawn(function()
    game:GetService("RunService").Heartbeat:Connect(function ()
        if _G.FastAttack then
            pcall(function ()
                
            end)
        end
        if _G.FastAttack then
            pcall(function()
                
            end)
        end
    end)
end)

Page5:Toggle("FastAttack", _G.FastAttack, function(x)
    _G.FastAttack = x
end)

local IslandCFrame = {
  Hee = CFrame.new(0, 0, 0)
}
local IslandName = {
  "Hee"
}
Page5:Dropdown("TP Island", "Selected", IslandName, function(v)
  SelectedIsland = v
end)
spawn(function()
  while task.wait() do
    if SelectedIsland == IslandName[1] then
      IslandCF = IslandCFrame[1]
    end
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = IslandCF
  end
end)
elseif Server == "Invaid Hwid !" then
game.Players.localPlayer:Kick("Invaid Hwid !")
elseif Server == "Invaid Key !" then
  game.Players.localPlayer:Kick("Invaid Key !")
end
